{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ablocks/menu-item",
  "version": "0.1.0",
  "title": "Menu Item",
  "category": "ablocks",
  "icon": "smiley",
  "description": "Show Submenu",
  "example": [],
  "parent": [
    "ablocks/menu",
    "ablocks/menu-child-sub"
  ],
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "ablocks",
  "style": [
    "file:./style.css"
  ],
  "viewScript": "file:./view.js",
  "attributes": {
    "block_id": {
      "type": "string",
      "default": ""
    },
    "label": {
      "type": "string",
      "default": "Menu"
    },
    "isSubMenu": {
      "type": "boolean",
      "default": false
    },
    "hasMegaMenu": {
      "type": "boolean",
      "default": false
    },
    "hasLink": {
      "type": "boolean",
      "default": false
    },
    "link": {
      "type": "object",
      "default": {
        "linkDestination": "",
        "href": "",
        "lightbox": "",
        "linkTarget": "",
        "rel": "",
        "noFollow": "",
        "keyValue": "",
        "linkClass": ""
      }
    }
  }
}